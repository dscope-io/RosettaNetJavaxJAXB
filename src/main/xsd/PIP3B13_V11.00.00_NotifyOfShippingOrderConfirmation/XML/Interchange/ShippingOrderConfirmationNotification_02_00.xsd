<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="urn:rosettanet:specification:interchange:ShippingOrderConfirmationNotification:xsd:schema:02.00"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:urss="urn:rosettanet:specification:system:xml:1.0"
    xmlns:tns="urn:rosettanet:specification:interchange:ShippingOrderConfirmationNotification:xsd:schema:02.00"
    xmlns:dl="urn:rosettanet:specification:domain:Logistics:xsd:schema:02.15"
    xmlns:sha="urn:rosettanet:specification:domain:Shared:xsd:schema:01.10"
    xmlns:rpkt="urn:rosettanet:specification:domain:Shared:PackageType:xsd:codelist:01.01"
    xmlns:udc="urn:rosettanet:specification:universal:Document:xsd:schema:01.08"
    xmlns:uuom="urn:rosettanet:specification:universal:UnitOfMeasure:xsd:codelist:01.03"
    xmlns:updi="urn:rosettanet:specification:universal:ProductIdentification:xsd:schema:01.04"
    xmlns:dsm="urn:rosettanet:specification:domain:Logistics:ShipmentMode:xsd:codelist:01.05"
    xmlns:upi="urn:rosettanet:specification:universal:PartnerIdentification:xsd:schema:01.12"
    xmlns:dp="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.17"
    xmlns:rssl="urn:rosettanet:specification:domain:Shared:ShippingServiceLevel:xsd:codelist:01.01"
    xmlns:doa="urn:rosettanet:specification:domain:Procurement:OrderAdmin:xsd:codelist:01.03"
    xmlns:ssdh="urn:rosettanet:specification:system:StandardDocumentHeader:xsd:schema:01.16" elementFormDefault="qualified" attributeFormDefault="unqualified" version="02.00">
    <xs:import namespace="urn:rosettanet:specification:domain:Logistics:xsd:schema:02.15" schemaLocation="../Domain/Logistics/Logistics_02_15.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Shared:xsd:schema:01.10" schemaLocation="../Domain/Shared/Shared_01_10.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Shared:PackageType:xsd:codelist:01.01" schemaLocation="../Domain/Shared/CodeList/RN_PackageType_01_01.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:Document:xsd:schema:01.08" schemaLocation="../Universal/Document_01_08.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:UnitOfMeasure:xsd:codelist:01.03" schemaLocation="../Universal/CodeList/RN_UnitOfMeasure_01_03.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:ProductIdentification:xsd:schema:01.04" schemaLocation="../Universal/ProductIdentification_01_04.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Logistics:ShipmentMode:xsd:codelist:01.05" schemaLocation="../Domain/Logistics/CodeList/RN_ShipmentMode_01_05.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:PartnerIdentification:xsd:schema:01.12" schemaLocation="../Universal/PartnerIdentification_01_12.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.17" schemaLocation="../Domain/Procurement/Procurement_02_17.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Shared:ShippingServiceLevel:xsd:codelist:01.01" schemaLocation="../Domain/Shared/CodeList/RN_ShippingServiceLevel_01_01.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Procurement:OrderAdmin:xsd:codelist:01.03" schemaLocation="../Domain/Procurement/CodeList/RN_OrderAdmin_01_03.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:system:StandardDocumentHeader:xsd:schema:01.16" schemaLocation="../System/StandardDocumentHeader_01_16.xsd"></xs:import>
