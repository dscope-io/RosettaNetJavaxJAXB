					<!--REM:  Success Condition for Notify Of Sales Report Acknowledgement-->
					<Success nameID="Success_ST" fromBusinessState='../BusinessTransactionActivity[@name="Notify Of Sales Report Acknowledgement"]' conditionGuard="Success">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Success"'/>
					</Success>
					<!--REM:  Failure Condition for Notify Of Sales Report Acknowledgement-->
					<Failure nameID="Failure_ST" fromBusinessState='../BusinessTransactionActivity[@name="Notify Of Sales Report Acknowledgement"]' conditionGuard="BusinessFailure">
