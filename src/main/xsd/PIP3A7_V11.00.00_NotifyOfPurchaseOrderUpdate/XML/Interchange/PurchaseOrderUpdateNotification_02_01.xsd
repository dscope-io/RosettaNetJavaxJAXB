	<xs:complexType name="ProductLineItemType">
		<xs:annotation>
			<xs:appinfo>
				<urss:Definition>The collection of business properties that describe a business document entry for a product. </urss:Definition>
				<urss:CreationDate>2005-04-18</urss:CreationDate>
				<urss:LastUpdatedDate>2006-05-23</urss:LastUpdatedDate>
				<urss:TypeVersion>02.01</urss:TypeVersion>
				<urss:Constraint>
					<st:ns prefix="dp" uri="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.06"/>
					<st:ns prefix="dsh" uri="urn:rosettanet:specification:domain:Procurement:SpecialHandling:xsd:codelist:01.03"/>
					<st:pattern name="specialHandlingInstruction">
						<st:rule context="tns:ProductLineItem">
							<st:assert test="(count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dp:Text) + count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dsh:SpecialHandling)) &gt;= 1">In ProductLineItem, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</st:assert>
						</st:rule>
					</st:pattern>
				</urss:Constraint>
			</xs:appinfo>
			<xs:documentation>
				<urss:Constraint>In ProductLineItem, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</urss:Constraint>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="dat:ActionType" minOccurs="0"/>
			<xs:element ref="udc:BusinessDocumentReference" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="BuyerLineItem" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Line number assigned to an item by the product buyer.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comments" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Free form textual description of a general nature.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="dp:ContractInformation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CountryOfOrigin" type="uc:CountryType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Country where product originates.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="dp:CustomerInformation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ExpediteReferenceIdentifier" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The unique identification number for the expediting of a product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Information" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>A free form textual description of information, relating to a product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="InstallAt" type="upi:SpecifiedPartnerDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The partner and/or location to which the product must be set up for use or service.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsContainsChanges" type="xs:boolean">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Indicates changes have been requested by the recipient of an existing purchase order.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsDropShip" type="xs:boolean">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Indicates whether the order is a drop shipment.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineNumber" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Number of the line in the document.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="dp:OrderQuantity"/>
			<xs:element ref="dp:OrderShippingInformation" minOccurs="0"/>
			<xs:element name="OriginalScheduledEvent" type="dp:TransportationEventType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The initial date a transportation event is scheduled to occur.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="updi:ProductIdentification"/>
			<xs:element name="ProductSubLineItem" type="tns:ProductSubLineItemType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="dpoar:PurchaseOrderAcknowledgmentReason" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="dpofp:PurchaseOrderFillPriority" minOccurs="0"/>
			<xs:element ref="dpos:PurchaseOrderStatus"/>
			<xs:element name="RequestedEvent" type="dp:TransportationEventType">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The date a transportation event is requested to occur.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestedShipFrom" type="ulc:LocationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The DUNS or DUNS + 4 number of the location where the product is requested to be shipped from.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestedUnitPrice" type="sha:MonetaryAmountType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The price requested for a unit of product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="RevisionNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>An incremental number used to identify changes.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduledEvent" type="dp:TransportationEventType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The date a transportation event is scheduled to occur.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipFrom" type="upi:SpecifiedPartnerDescriptionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The DUNS or DUNS + 4 number of the location from where the product is to be shipped.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="dp:ShippedQuantityInformation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ShipTo" type="upi:SpecifiedPartnerDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The partner and/or location to which the product must be delivered. </urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="dp:SubstituteProductReference" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="dp:TaxExemptStatus" minOccurs="0"/>
			<xs:element ref="dp:TaxSummary" minOccurs="0"/>
			<xs:element name="TotalLineItemAmount" type="sha:MonetaryAmountType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The monetary total associated with a line item.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="uuom:UnitOfMeasure"/>
			<xs:element name="UnitPrice" type="sha:MonetaryAmountType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Agreed upon sale price for each unit ordered.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:token"/>
	</xs:complexType>
