on base="xs:token"><xs:enumeration value="ACQ"><xs:annotation><xs:appinfo><urss:Definition>Acquisition. Part has not been qualified but must be used in manufacturing (i.e. customer requests use of an unqualified part).</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="ABO"><xs:annotation><xs:appinfo><urss:Definition>Active by order. Part is available only by order and is not stocked.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="AIS"><xs:annotation><xs:appinfo><urss:Definition>Active in stock. Part is available immediately from stock.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="ALL"><xs:annotation><xs:appinfo><urss:Definition>Allocation. Part is under allocation.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="APP"><xs:annotation><xs:appinfo><urss:Definition>Approved. Part is approved.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="CON"><xs:annotation><xs:appinfo><urss:Definition>Conditional. Part has not been qualified but is used under certain conditions.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="COO"><xs:annotation><xs:appinfo><urss:Definition>Consigned. Part is supplied by vender or customer thereby denoting that the part is not purchased but consigned.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="DIS"><xs:annotation><xs:appinfo><urss:Definition>Disqualified. Part may not be used.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="NON"><xs:annotation><xs:appinfo><urss:Definition>Non-preferred. Part should not be used due to business reasons (i.e. cost/pricing).</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="OBS"><xs:annotation><xs:appinfo><urss:Definition>Obsolete. Part is approved but is no longer being produced.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="QHO"><xs:annotation><xs:appinfo><urss:Definition>Quality hold. Part is held for quality issues which must be reconciled before the part can be used in production.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="REF"><xs:annotation><xs:appinfo><urss:Definition>Reference. Part is used only for reference.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="UQU"><xs:annotation><xs:appinfo><urss:Definition>Under qualification. Part is under qualification.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration><xs:enumeration value="UNQ"><xs:annotation><xs:appinfo><urss:Definition>Unqualified. Part has not been qualified.</urss:Definition></xs:appinfo></xs:annotation></xs:enumeration></xs:restriction></xs:simpleType><xs:complexType name="PartStatusType"><xs:annotation><xs:appinfo><urss:CreationDate>2012-05-14</urss:CreationDate><urss:Identifier>PartStatusType</urss:Identifier><urss:LastUpdatedDate>2012-05-14</urss:LastUpdatedDate><urss:RegisteredBy>RosettaNet</urss:RegisteredBy><urss:TypeVersion>01.00</urss:TypeVersion></xs:appinfo></xs:annotation><xs:simpleContent><xs:extension base="tns:PartStatusContentType"><xs:attribute name="identifier" type="xs:token" fixed="PartStatus"></xs:attribute><xs:attribute name="codeListVersion" type="xs:token" fixed="01.00"></xs:attribute><xs:attribute name="agency" type="xs:token" fixed="RosettaNet"></xs:attribute><xs:attribute name="schemaVersion" type="xs:token"></xs:attribute></xs:extension></xs:simpleContent></xs:complexType></xs:schema>
