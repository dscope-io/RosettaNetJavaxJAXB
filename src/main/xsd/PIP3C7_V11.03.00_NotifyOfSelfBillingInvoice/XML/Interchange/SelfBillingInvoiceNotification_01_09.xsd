ionType"><xs:annotation><xs:appinfo><urss:Definition>The partner to which payment for a business document is sent.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element name="RemitToAccount" type="dp:AccountSummaryType" maxOccurs="unbounded" minOccurs="0"><xs:annotation><xs:appinfo><urss:Definition>The account number of the party to whom payment is sent.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element name="RemitToTax" type="dp:TaxSummaryType" minOccurs="0"><xs:annotation><xs:appinfo><urss:Definition>The total tax liability of the party to whom payment is sent.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element ref="dslt:SaleType" minOccurs="0"></xs:element><xs:element name="SelfBillingFrom" type="upi:SpecifiedPartnerDescriptionType"><xs:annotation><xs:appinfo><urss:Definition>The party who initiates the billing in self-billing scenario.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element name="SelfBillingTo" type="upi:SpecifiedPartnerDescriptionType"><xs:annotation><xs:appinfo><urss:Definition>The party whom the invoice is addressed to.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element name="SoldToAccount" type="xs:string" maxOccurs="unbounded" minOccurs="0"><xs:annotation><xs:appinfo><urss:Definition>The account number of the party that purchases of the product.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element name="SoldToTax" type="dp:TaxSummaryType" minOccurs="0"><xs:annotation><xs:appinfo><urss:Definition>The total tax liability of the party that purchases the product.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element name="TotalInvoiceAmount" type="sha:MonetaryAmountType"><xs:annotation><xs:appinfo><urss:Definition>The total amount on an invoice.</urss:Definition></xs:appinfo></xs:annotation></xs:element><xs:element name="TransferToAccount" type="xs:string" maxOccurs="unbounded" minOccurs="0"><xs:annotation><xs:appinfo>