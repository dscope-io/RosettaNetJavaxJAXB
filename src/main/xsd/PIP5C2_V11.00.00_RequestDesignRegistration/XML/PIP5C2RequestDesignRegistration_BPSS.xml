					<Failure nameID="Failure_ST" fromBusinessState='../BusinessTransactionActivity[@name="Request Design Registration"]' conditionGuard="BusinessFailure">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Failure"'/>
					</Failure>
				</BinaryCollaboration>
			</Package>
		</Package>
	</Package>
</ProcessSpecification>
