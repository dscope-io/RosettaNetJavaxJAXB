					<Success nameID="Success_ST" fromBusinessState='../BusinessTransactionActivity[@name="RequestDesignRegistration"]' conditionGuard="Success">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Success"'/>
					</Success>
