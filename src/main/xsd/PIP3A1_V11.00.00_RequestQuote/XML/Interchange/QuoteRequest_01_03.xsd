	<xs:import namespace="urn:rosettanet:specification:domain:Procurement:GovernmentPriorityRating:xsd:codelist:01.02" schemaLocation="../Domain/Procurement/CodeList/RN_GovernmentPriorityRating_01_02.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:UnitOfMeasure:xsd:codelist:01.01" schemaLocation="../Universal/CodeList/RN_UnitOfMeasure_01_01.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:interchange:StockIndicator:xsd:codelist:01.00" schemaLocation="CodeList/RN_StockIndicator_01_00.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:domain:Procurement:QuoteStatus:xsd:codelist:01.01" schemaLocation="../Domain/Procurement/CodeList/RN_QuoteStatus_01_01.xsd"/>
