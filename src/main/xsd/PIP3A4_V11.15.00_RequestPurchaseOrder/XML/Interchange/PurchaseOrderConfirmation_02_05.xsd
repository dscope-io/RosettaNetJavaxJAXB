    <xs:import namespace="urn:rosettanet:specification:universal:MonetaryExpression:xsd:schema:01.06" schemaLocation="../Universal/MonetaryExpression_01_06.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Procurement:PurchaseOrderFillPriority:xsd:codelist:01.03" schemaLocation="../Domain/Procurement/CodeList/RN_PurchaseOrderFillPriority_01_03.xsd"></xs:import>
