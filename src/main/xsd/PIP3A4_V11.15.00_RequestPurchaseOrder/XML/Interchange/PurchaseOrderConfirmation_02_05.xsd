    <xs:import namespace="urn:rosettanet:specification:domain:Procurement:PurchaseOrderStatus:xsd:codelist:01.06" schemaLocation="../Domain/Procurement/CodeList/RN_PurchaseOrderStatus_01_06.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Procurement:PurchaseOrderType:xsd:codelist:01.05" schemaLocation="../Domain/Procurement/CodeList/RN_PurchaseOrderType_01_05.xsd"></xs:import>
