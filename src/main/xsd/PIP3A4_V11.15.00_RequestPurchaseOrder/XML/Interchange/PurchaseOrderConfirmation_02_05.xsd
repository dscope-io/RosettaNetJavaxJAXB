    <xs:element name="PurchaseOrderConfirmation" type="tns:PurchaseOrderConfirmationType"></xs:element>
    <xs:complexType name="OrderLineItemType">
        <xs:annotation>
            <xs:appinfo>
                <urss:Definition>The collection of business properties that describe a entry in a purchase order business document.</urss:Definition>
                <urss:CreationDate>2005-09-14</urss:CreationDate>
                <urss:LastUpdatedDate>2007-07-30</urss:LastUpdatedDate>
                <urss:TypeVersion>02.02</urss:TypeVersion>
            </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
            <xs:choice>
                <xs:element name="ProductLineItem" type="tns:ProductLineItemType"></xs:element>
                <xs:element name="ServiceLineItem" type="tns:ServiceLineItemType"></xs:element>
                <xs:element name="ServiceLineItemByOption" type="tns:ServiceLineItemByOptionType"></xs:element>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="schemaVersion" type="xs:token"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="ProductLineItemType">
        <xs:annotation>
            <xs:appinfo>
                <urss:Definition>The collection of business properties that describe a business document entry for a product in a purchase order business documents. The Recommended ProcessRoleIdentifier for PartnerDescription are CLF - Collect From, SHF - Ship From and SHT -Ship To.</urss:Definition>
                <urss:CreationDate>2005-04-05</urss:CreationDate>
                <urss:LastUpdatedDate>2007-07-30</urss:LastUpdatedDate>
                <urss:TypeVersion>02.02</urss:TypeVersion>
                <urss:Constraint>
                    <st:ns prefix="dp" uri="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.29"/>
                    <st:ns prefix="dsh" uri="urn:rosettanet:specification:domain:Procurement:SpecialHandling:xsd:codelist:01.04"/>
                    <st:pattern name="specialHandling">
                        <st:rule context="tns:ProductLineItem">
                            <st:assert test="(count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dp:Text) + count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dsh:SpecialHandling)) &gt;= 1">In ProductLineItem, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</st:assert>
                        </st:rule>
                    </st:pattern>
                </urss:Constraint>
            </xs:appinfo>
            <xs:documentation>
                <urss:Constraint>In ProductLineItem, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</urss:Constraint>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="dp:AncillaryDocument" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="udc:BusinessDocumentReference" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="BuyerLineItem" type="xs:string">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Line number assigned to an item by the product buyer.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="Comments" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Free form textual description of a general nature.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:ContractInformation" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="CountryOfOrigin" type="uc:CountryType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Country where product originates.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:CustomerInformation" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="ExpediteReferenceIdentifier" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The unique identification number for the expediting of a product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="Identifier" type="xs:string" maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>A unique serial identifier assigned to a product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="Information" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>A free form textual description of information, relating to a product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="InstallAtPartnerLine" type="upi:SpecifiedNewPartnerType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The partner and/or location to which the product must be set up for use or service.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="IsDropShip" type="xs:boolean">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Indicates whether the order is a drop shipment.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="LineNumber" type="xs:string">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Number of the line in the document.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="ListPrice" type="sha:ProductPricingType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>This object represents the pre-discounted price.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:MapReference" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:MilestoneDate" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:OrderQuantity"></xs:element>
            <xs:element ref="dp:OrderShippingInformation" minOccurs="0"></xs:element>
            <xs:element ref="upi:PartnerDescription" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:PortalAccess" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="updi:ProductIdentification"></xs:element>
            <xs:element name="ProductSubLineItem" type="tns:ProductSubLineItemType" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dpoar:PurchaseOrderAcknowledgmentReason" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dpofp:PurchaseOrderFillPriority" minOccurs="0"></xs:element>
            <xs:element ref="dpos:PurchaseOrderStatus"></xs:element>
            <xs:element name="RequestedEvent" type="dp:TransportationEventType">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The date a transportation event is requested to occur.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="RequestedShipFrom" type="ulc:LocationType" maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The DUNS or DUNS + 4 number of the location where the product is requested to be shipped from.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="RequestedUnitPrice" type="sha:MonetaryAmountType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The price requested for a unit of product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="ResponseDescription" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>This object representing the description of response.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="ScheduledEvent" type="dp:TransportationEventType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The date a transportation event is scheduled to occur.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:ShippedQuantityInformation" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:SpecialDiscount" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:SubstituteProductReference" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:TaxExemptStatus" minOccurs="0"></xs:element>
            <xs:element name="TaxSummary" type="tns:TaxSummaryType" minOccurs="0"></xs:element>
            <xs:element name="TotalLineItemAmount" type="sha:MonetaryAmountType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The monetary total associated with a line item.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="uuom:UnitOfMeasure"></xs:element>
            <xs:element name="UnitPrice" type="sha:MonetaryAmountType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Agreed upon sale price for each unit ordered.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="schemaVersion" type="xs:token"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="ProductSubLineItemType">
        <xs:annotation>
            <xs:appinfo>
                <urss:Definition>The collection of business properties that describe a part of a product line item. The Recommended ProcessRoleIdentifier for PartnerDescription are CLF - Collect From.</urss:Definition>
                <urss:CreationDate>2005-04-05</urss:CreationDate>
                <urss:LastUpdatedDate>2007-07-30</urss:LastUpdatedDate>
                <urss:TypeVersion>02.02</urss:TypeVersion>
                <urss:Constraint>
                    <st:ns prefix="dp" uri="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.29"/>
                    <st:ns prefix="dsh" uri="urn:rosettanet:specification:domain:Procurement:SpecialHandling:xsd:codelist:01.04"/>
                    <st:pattern name="specialHandling">
                        <st:rule context="tns:ProductSubLineItem">
                            <st:assert test="(count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dp:Text) + count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dsh:SpecialHandling)) &gt;= 1">In ProductSubLineItem, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</st:assert>
                        </st:rule>
                    </st:pattern>
                </urss:Constraint>
            </xs:appinfo>
            <xs:documentation>
                <urss:Constraint>In ProductSubLineItem, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</urss:Constraint>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="dp:AncillaryDocument" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="BuyerSubLineItem" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition> The information related to the original buyer submitted document and contained within the subline level.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="Comments" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Free form textual description of a general nature.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:ContractInformation" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="CountryOfOrigin" type="uc:CountryType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Country where product originates.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:CustomerInformation" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="ExpediteReferenceIdentifier" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The unique identification number for the expediting of a product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="Identifier" type="xs:string" maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>A unique serial identifier assigned to a product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="Information" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>A free form textual description of information, relating to a product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="InstallAtSubLine" type="upi:SpecifiedKnownPartnerContactType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The partner and/or location to which the product must be set up for use or service.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="IsDropShip" type="xs:boolean">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Indicates whether the order is a drop shipment.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:MapReference" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:MilestoneDate" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dp:OrderQuantity"></xs:element>
            <xs:element ref="dp:OrderShippingInformation" minOccurs="0"></xs:element>
            <xs:element ref="upi:PartnerDescription" minOccurs="0"></xs:element>
            <xs:element ref="dp:PortalAccess" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dpoar:PurchaseOrderAcknowledgmentReason" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element ref="dpofp:PurchaseOrderFillPriority" minOccurs="0"></xs:element>
            <xs:element ref="dpos:PurchaseOrderStatus"></xs:element>
            <xs:element name="RequestedEvent" type="dp:TransportationEventType">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The date a transportation event is requested to occur.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="RequestedShipFrom" type="ulc:LocationType" maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The DUNS or DUNS + 4 number of the location where the product is requested to be shipped from.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="ResponseDescription" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>This object representing the description of response.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:ShippedQuantityInformation" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="ShipToPartnerSubLine" type="upi:SpecifiedFullPartnerType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The partner and/or location to which the product must be delivered.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dp:SpecialDiscount" maxOccurs="unbounded" minOccurs="0"></xs:element>
            <xs:element name="SubLineItem" type="xs:string">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>Information contained within a subline.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element ref="uuom:UnitOfMeasure"></xs:element>
            <xs:element name="UnitPrice" type="sha:MonetaryAmountType" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <urss:Definition>The price for a unit of product.</urss:Definition>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="schemaVersion" type="xs:token"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="PurchaseOrderType">
        <xs:annotation>
            <xs:appinfo>
                <urss:Definition>The collection of business properties that describe a buyer's offer to purchase a quantity of products at an agreed price and schedule. The Recommended ProcessRoleIdentifier for PartnerDescription are ITL - Install At, BIT - Bill To, CWN - Contract Owner, SHT - Ship To, RES - Respond To, SHF - Ship From and CLF - Collect From.</urss:Definition>
                <urss:CreationDate>2005-04-05</urss:CreationDate>
                <urss:LastUpdatedDate>2009-07-23</urss:LastUpdatedDate>
                <urss:TypeVersion>02.03</urss:TypeVersion>
                <urss:Constraint>
                    <st:ns prefix="dp" uri="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.29"/>
                    <st:pattern name="netPayment">
                        <st:rule context="tns:PurchaseOrder">
                            <st:assert test="(count(dp:FinancingTerms/dp:PaymentTerms/dp:NetTerms/dp:Day) + count(dp:FinancingTerms/dp:PaymentTerms/dp:NetTerms/dp:Days)) &gt;= 1">In PurchaseOrder, at least one occurrence of FinancingTerms/PaymentTerms/NetTerms/Day or FinancingTerms/PaymentTerms/NetTerms/Days.</st:assert>
                        </st:rule>
                    </st:pattern>
                </urss:Constraint>
                <urss:Constraint>
                    <st:ns prefix="dp" uri="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.29"/>
                    <st:ns prefix="dsh" uri="urn:rosettanet:specification:domain:Procurement:SpecialHandling:xsd:codelist:01.04"/>
                    <st:pattern name="specialHandling">
                        <st:rule context="tns:PurchaseOrder">
                            <st:assert test="(count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dp:Text) + count(dp:OrderShippingInformation/dp:SpecialHandlingInstruction/dsh:SpecialHandling)) &gt;= 1">In PurchaseOrder, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</st:assert>
                        </st:rule>
                    </st:pattern>
                </urss:Constraint>
            </xs:appinfo>
            <xs:documentation>
                <urss:Constraint>In PurchaseOrder, at least one occurrence of FinancingTerms/PaymentTerms/NetTerms/Day or FinancingTerms/PaymentTerms/NetTerms/Days.</urss:Constraint>
                <urss:Constraint>In PurchaseOrder, at least one occurrence of OrderShippingInformation/SpecialHandlingInstruction/Text or OrderShippingInformation/SpecialHandlingInstruction/SpecialHandling.</urss:Constraint>
