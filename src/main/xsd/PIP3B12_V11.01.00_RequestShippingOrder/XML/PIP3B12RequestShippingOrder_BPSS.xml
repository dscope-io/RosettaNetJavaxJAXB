<!--Compliant with BPSS vers 1.01-->
<ProcessSpecification name="PIP3B12RequestShippingOrder" nameID="urn:rosettanet:specification:interchange:PIP3B12:xml:bpss:v11_01" version="V11.01" xmlns="http://www.ebxml.org/BusinessProcess/1.01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ebxml.org/BusinessProcess ebBPSS1.01.xsd">
	<Documentation>This PIP is designed to enable the communication of shipping orders from a manufacturer or distributor with outsourced logistics operations to its third-party logistics provider.</Documentation>
	<!--REM: Shipping Order Request Document-->
	<BusinessDocument name="Shipping Order Request" nameID="PIP3B12ShippingOrderRequest" specificationLocation="urn:rosettanet:specification:interchange:ShippingOrderRequest:xsd:schema:02.01">
		<Documentation>Contains information requesting the shipment of a product or products.</Documentation>
	</BusinessDocument>
	<!--REM: Shipping Order Confirmation Document-->
	<BusinessDocument name="Shipping Order Confirmation" nameID="PIP3B12ShippingOrderConfirmation" specificationLocation="urn:rosettanet:specification:interchange:ShippingOrderConfirmation:xsd:schema:02.01">
		<Documentation>Contains either an acceptance or rejection of the shipping order request, with status information to support this.</Documentation>
	</BusinessDocument>
	<Package name="Order Management" nameID="3">
		<Documentation>Defines Cluster</Documentation>
		<Package name="Transportation and Distribution" nameID="3B">
			<Documentation>Defines Segment</Documentation>
			<Package name="Request Shipping Order" nameID="3B12">
				<Documentation>Defines PIP</Documentation>
				<BusinessTransaction name="Request Shipping Order" nameID="RequestShippingOrder_BT" isGuaranteedDeliveryRequired="true">
					<RequestingBusinessActivity name="Initiate Shipping Order Request" nameID="InitiateShippingOrderRequest" isAuthorizationRequired="true" isIntelligibleCheckRequired="true" isNonRepudiationReceiptRequired="true" isNonRepudiationRequired="true" timeToAcknowledgeReceipt="PT2H" retryCount="3">
						<DocumentEnvelope name="Shipping Order Request" nameID="ShippingOrderRequest_DE" businessDocument='//BusinessDocument[@name="Shipping Order Request"]' businessDocumentIDREF="PIP3B12ShippingOrderRequest" isAuthenticated="persistent" isConfidential="transient" isTamperDetectable="persistent"/>
					</RequestingBusinessActivity>
					<RespondingBusinessActivity name="Initiate Shipping Order Confirmation" nameID="InitiateShippingOrderConfirmation" isAuthorizationRequired="true" isIntelligibleCheckRequired="true" isNonRepudiationReceiptRequired="true" isNonRepudiationRequired="true" timeToAcknowledgeReceipt="PT2H">
						<DocumentEnvelope name="Shipping Order Confirmation" nameID="ShippingOrderConfirmation_DE" businessDocument='//BusinessDocument[@name="Shipping Order Confirmation"]' businessDocumentIDREF="PIP3B12ShippingOrderConfirmation" isAuthenticated="persistent" isConfidential="transient" isPositiveResponse="true" isTamperDetectable="persistent"/>
					</RespondingBusinessActivity>
				</BusinessTransaction>
				<!--REM: Binary Collaborations-->
				<BinaryCollaboration name="Request Shipping Order" nameID="RequestShippingOrder_BC" initiatingRoleIDREF="ShipperID" preCondition="true" postCondition="true">
					<Documentation>The PIP Start State is comprised of the following conditions: a. TPA Approved and Exist In Backend System b. Sending Partner Exist as Party and Valid c. Receiving Partner Exist as Party d. Business Document Exist In Backend System, Valid, Non-Repudiated and Authorized </Documentation>
					<Documentation>The PIP End States are comprised of the following conditions: a. Acknowledgment of Receipt Exist In Backend System, Valid and Non-Repudiated b. Receiving Partner Authorized ; PIP Failure State: a. NOT Successful b. Retry Count Exceeded</Documentation>
					<Role name="Shipper" nameID="ShipperID"/>
					<Role name="Shipping Provider" nameID="ShippingProviderID"/>
					<Start nameID="Start_ST" toBusinessState='../BusinessTransactionActivity[@name="Request Shipping Order"]' toBusinessStateIDREF="PIP3B12RequestShippingOrder"/>
					<BusinessTransactionActivity name="Request Shipping Order" nameID="RequestShippingOrder_BTA" businessTransaction='//BusinessTransaction[@name="Request Shipping Order"]' businessTransactionIDREF="RequestShippingOrder_BT" fromRole='../Role[@name="Shipper"]' fromRoleIDREF="ShipperID" toRole='../Role[@name="Shipping Provider"]' toRoleIDREF="ShippingProviderID" isLegallyBinding="true" isConcurrent="false" timeToPerform="PT4H"/>
					<!--REM: The following shows a business transaction with two (one-pair) possible responses, each pair with a success and a failure.These are designed to reflect 
					business level conditions only and not message level conditions such as Receipt Acknowledgements (ReceiptAck) and/or Notification of Failures (NoF). -->
					<!--REM:  Success Condition for Request Shipping Order-->
					<Success nameID="Success_ST" fromBusinessState='../BusinessTransactionActivity[@name="Request Shipping Order"]' conditionGuard="Success">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Success"'/>
					</Success>
					<!--REM: Failure Condition for Request Shipping Order-->
					<Failure nameID="Failure_ST" fromBusinessState='../BusinessTransactionActivity[@name="Request Shipping Order"]' conditionGuard="BusinessFailure">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Failure"'/>
					</Failure>
				</BinaryCollaboration>
			</Package>
		</Package>
	</Package>
</ProcessSpecification>
