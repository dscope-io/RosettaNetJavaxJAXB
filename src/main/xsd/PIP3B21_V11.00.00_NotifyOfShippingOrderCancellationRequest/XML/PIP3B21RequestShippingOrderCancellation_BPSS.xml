<ProcessSpecification name="PIP3B21NotifyOfShippingOrderCancellationRequest" nameID="urn:rosettanet:specification:interchange:PIP3B21:xml:bpss:v11_00" version="V11.00" xmlns="http://www.ebxml.org/BusinessProcess/1.01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ebxml.org/BusinessProcess/1.01 ebBPSS1.01.xsd">
	<Documentation>This PIP supports a process between two of these parties, in which a Shipper cancels a shipping order that has been sent previously to a Shipping Provider.</Documentation>
	<BusinessDocument name="Shipping Order Cancellation Request" nameID="PIP3B21ShippingOrderCancellationRequest" specificationLocation="urn:rosettanet:specification:interchange:ShippingOrderCancellationRequest:xsd:schema:01.00">
		<Documentation>Contains information requesting cancellation of a previously requested shipping order</Documentation>
	</BusinessDocument>
	<BusinessDocument name="Shipping Order Cancellation Notification Request" nameID="PIP3B21ShippingOrderCancellationNotificationRequest" specificationLocation="urn:rosettanet:specification:interchange:ShippingOrderCancellationNotificationRequest:xsd:schema:02.02">
		<Documentation>Contains either an acceptance or rejection of the shipping order cancellation request, with status information to support this</Documentation>
	</BusinessDocument>
	<Package name="Order Management" nameID="3">
		<Documentation>Defines Cluster</Documentation>
		<Package name="Transportation and Distribution" nameID="3B">
			<Documentation>Defines Segment</Documentation>
			<Package name="NotifyOfShippingOrderCancellationRequest" nameID="3B21">
				<Documentation>Defines PIP</Documentation>
				<BusinessTransaction name="Notify of Shipping Order Cancellation Request" nameID="NotifyOfShippingOrderCancellationRequest_BT" isGuaranteedDeliveryRequired="true">
					<RequestingBusinessActivity name="Initiate Shipping Order Cancellation Request" nameID="InitiateShippingOrderCancellationRequest" isAuthorizationRequired="true" isIntelligibleCheckRequired="true" isNonRepudiationReceiptRequired="true" isNonRepudiationRequired="true" timeToAcknowledgeReceipt="PT2H" retryCount="3">
