					<Success nameID="Success_ST" fromBusinessState='../BusinessTransactionActivity[@name="Distribute Registration Status"]' conditionGuard="Success">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Success"'/>
					</Success>
