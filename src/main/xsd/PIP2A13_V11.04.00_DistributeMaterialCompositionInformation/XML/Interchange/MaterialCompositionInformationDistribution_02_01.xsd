<xs:schema targetNamespace="urn:rosettanet:specification:interchange:MaterialCompositionInformationDistribution:xsd:schema:02.01" xmlns:uci="urn:rosettanet:specification:universal:ContactInformation:xsd:schema:01.03" xmlns:udc="urn:rosettanet:specification:universal:Document:xsd:schema:01.08" xmlns:ssdh="urn:rosettanet:specification:system:StandardDocumentHeader:xsd:schema:01.18" xmlns:upi="urn:rosettanet:specification:universal:PartnerIdentification:xsd:schema:01.14" xmlns:dds="urn:rosettanet:specification:domain:Design:xsd:schema:02.17" xmlns:tns="urn:rosettanet:specification:interchange:MaterialCompositionInformationDistribution:xsd:schema:02.01" xmlns:urss="urn:rosettanet:specification:system:xml:1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="02.01">
	<xs:import namespace="urn:rosettanet:specification:domain:Design:xsd:schema:02.17" schemaLocation="../Domain/Design/Design_02_17.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:PartnerIdentification:xsd:schema:01.14" schemaLocation="../Universal/PartnerIdentification_01_14.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:system:StandardDocumentHeader:xsd:schema:01.18" schemaLocation="../System/StandardDocumentHeader_01_18.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:Document:xsd:schema:01.08" schemaLocation="../Universal/Document_01_08.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:ContactInformation:xsd:schema:01.03" schemaLocation="../Universal/ContactInformation_01_03.xsd"/>
	<xs:annotation>
