<xs:schema targetNamespace="urn:rosettanet:specification:interchange:ProductChangeNotification:xsd:schema:01.00" xmlns:ssdh="urn:rosettanet:specification:system:StandardDocumentHeader:xsd:schema:01.15" xmlns:dnc="urn:rosettanet:specification:domain:Service:NotificationCode:xsd:codelist:01.03" xmlns:dds="urn:rosettanet:specification:domain:Design:xsd:schema:02.12" xmlns:dm="urn:rosettanet:specification:domain:Manufacturing:xsd:schema:02.15" xmlns:upi="urn:rosettanet:specification:universal:PartnerIdentification:xsd:schema:01.11" xmlns:updi="urn:rosettanet:specification:universal:ProductIdentification:xsd:schema:01.04" xmlns:rpkt="urn:rosettanet:specification:domain:Shared:PackageType:xsd:codelist:01.01" xmlns:udc="urn:rosettanet:specification:universal:Document:xsd:schema:01.08" xmlns:tns="urn:rosettanet:specification:interchange:ProductChangeNotification:xsd:schema:01.00" xmlns:urss="urn:rosettanet:specification:system:xml:1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="01.00">
	<xs:import namespace="urn:rosettanet:specification:universal:Document:xsd:schema:01.08" schemaLocation="../Universal/Document_01_08.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:domain:Shared:PackageType:xsd:codelist:01.01" schemaLocation="../Domain/Shared/CodeList/RN_PackageType_01_01.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:ProductIdentification:xsd:schema:01.04" schemaLocation="../Universal/ProductIdentification_01_04.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:universal:PartnerIdentification:xsd:schema:01.11" schemaLocation="../Universal/PartnerIdentification_01_11.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:domain:Manufacturing:xsd:schema:02.15" schemaLocation="../Domain/Manufacturing/Manufacturing_02_15.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:domain:Design:xsd:schema:02.12" schemaLocation="../Domain/Design/Design_02_12.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:domain:Service:NotificationCode:xsd:codelist:01.03" schemaLocation="../Domain/Service/CodeList/RN_NotificationCode_01_03.xsd"/>
	<xs:import namespace="urn:rosettanet:specification:system:StandardDocumentHeader:xsd:schema:01.15" schemaLocation="../System/StandardDocumentHeader_01_15.xsd"/>
	<xs:annotation>
