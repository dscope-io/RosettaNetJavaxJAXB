			<urss:Program>Product Change Notification Milestone Program</urss:Program>
			<urss:Purpose>Provides the RosettaNet 2B7 Product Change Notification classes.</urss:Purpose>
		</xs:documentation>
		<xs:appinfo>
			<urss:CreationDate>2006-11-20</urss:CreationDate>
			<urss:LastUpdatedDate>2006-11-20</urss:LastUpdatedDate>
		</xs:appinfo>
	</xs:annotation>
	<xs:element name="ProductChangeNotification" type="tns:ProductChangeNotificationType"/>
	<xs:complexType name="DocumentChangedType">
		<xs:annotation>
			<xs:appinfo>
				<urss:Definition>A business property that describes document change in product change or end of life.</urss:Definition>
				<urss:CreationDate>2006-11-20</urss:CreationDate>
				<urss:LastUpdatedDate>2006-11-20</urss:LastUpdatedDate>
				<urss:TypeVersion>01.00</urss:TypeVersion>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IsDocumentChanged" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents whether there are document Does the change, as described, result in a change to the supplier datasheet. Yes/No.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="RelatedDocument" type="udc:BusinessDocumentReferenceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The object represent the list of documents which are affected by the change.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:token"/>
	</xs:complexType>
	<xs:complexType name="ProductChangeType">
		<xs:annotation>
			<xs:appinfo>
				<urss:Definition>The collection of business properties that describe the nature and contents of a product change.</urss:Definition>
				<urss:CreationDate>2006-11-20</urss:CreationDate>
				<urss:LastUpdatedDate>2006-11-20</urss:LastUpdatedDate>
				<urss:TypeVersion>01.00</urss:TypeVersion>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AffectedCustomerProduct" type="tns:ProductDescriptionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Customer part number of affected product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="AffectedProduct" type="tns:ProductDescriptionType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Affected product refers to the supplier.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="dm:Attachment" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="udc:BusinessDocumentReference"/>
			<xs:element ref="dm:ChangeDescription" minOccurs="0"/>
			<xs:element ref="dds:DateReference" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="DocumentChanged" type="tns:DocumentChangedType" minOccurs="0"/>
			<xs:element ref="dnc:NotificationCode" minOccurs="0"/>
			<xs:element name="PreviousDocumentReference" type="udc:BusinessDocumentReferenceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Reference to related notifications and previously published notices.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductIdentificationMethod" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents the method of identifiying change product by date code, lot trace code, part numbe or other nomenclature change that allows customer to distinguish change product from unchanged.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="QualData" type="tns:QualDataType" minOccurs="0"/>
			<xs:element name="ReplacementProduct" type="tns:ProductDescriptionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>Replacement parts for the products affected by the notice.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="SuppliedBy" type="upi:SpecifiedFullPartnerType">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The party responsible for supplying the product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:token"/>
	</xs:complexType>
	<xs:complexType name="ProductChangeNotificationType">
		<xs:annotation>
			<xs:appinfo>
				<urss:Definition>The collection of business properties that describe the changes made to a product/process. </urss:Definition>
				<urss:CreationDate>2006-11-20</urss:CreationDate>
				<urss:LastUpdatedDate>2006-11-20</urss:LastUpdatedDate>
				<urss:TypeVersion>01.00</urss:TypeVersion>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ssdh:DocumentHeader" minOccurs="0"/>
			<xs:element name="ProductChange" type="tns:ProductChangeType"/>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:token"/>
	</xs:complexType>
	<xs:complexType name="ProductDescriptionType">
		<xs:annotation>
			<xs:appinfo>
				<urss:Definition>The collection of business properties that describe the nature and contents of a product descriptions.</urss:Definition>
				<urss:CreationDate>2006-11-20</urss:CreationDate>
				<urss:LastUpdatedDate>2006-11-20</urss:LastUpdatedDate>
				<urss:TypeVersion>01.00</urss:TypeVersion>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Category" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents the which category or group of the product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeliveryHistory" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents the history/summary of information regarding number of units purchased in last two years can help customer with analysis of PCN or EOL.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Line" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents a line or families of a product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ManufacturedBy" type="upi:SpecifiedFullPartnerType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>The party responsible for manufactured the product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="rpkt:PackageType" minOccurs="0"/>
			<xs:element name="PartNumber" type="updi:ProductIdentificationType">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>A unique part number to identify a product.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:token"/>
	</xs:complexType>
	<xs:complexType name="QualDataType">
		<xs:annotation>
			<xs:appinfo>
				<urss:Definition>The collection of qual data sent by supplier  to demonstrate conclusively that the product or process change in question does not adversely impact the reliability or the functionality of the device.</urss:Definition>
				<urss:CreationDate>2006-11-20</urss:CreationDate>
				<urss:LastUpdatedDate>2006-11-20</urss:LastUpdatedDate>
				<urss:TypeVersion>01.00</urss:TypeVersion>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Plan" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents the qualification plan of a product change.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="QualificationStatus" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents the summary of the current status of the qualification of the change.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Result" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents the qualification results of a product change.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Schedule" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<urss:Definition>This object represents the qualification schedule of a product change.</urss:Definition>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:token"/>
	</xs:complexType>
</xs:schema>
