<!--Compliant with BPSS vers 1.01-->
<ProcessSpecification name="PIP4C1DistributeInventoryReport" nameID="urn:rosettanet:specification:interchange:PIP4C1:xml:bpss:v11_01" version="V11.01" xmlns="http://www.ebxml.org/BusinessProcess/1.01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ebxml.org/BusinessProcess/1.01 ebBPSS1.01.xsd">
	<Documentation>The purpose of this PIP is to 1) automate the distribution of inventory report information exchanges between the supply chain partners, 2) improve information exchange quality and efficiency when gathering inventory report data, 3) save time and reduce processing costs, 4) reduce workload in supply chain by introducing a common global approach for sharing inventory report data across the full high-tech supply chain.</Documentation>
	<!--REM: Inventory Report Distribution Document-->
	<BusinessDocument name="Inventory Report Distribution" nameID="PIP4C1InventoryReportDistribution" specificationLocation="urn:rosettanet:specification:interchange:InventoryReportDistribution:xsd:schema:02.01">
		<Documentation>The document is an XML Schema file that specifies the rules for creating the XML document for the distribution business action of inventory report. The specificationLocation attribute is used for illustrative purposes only.</Documentation>
	</BusinessDocument>
	<Package name="Inventory Management" nameID="4">
		<Documentation>Defines Cluster</Documentation>
		<Package name="Inventory Reporting" nameID="4C">
			<Documentation>Defines Segment</Documentation>
			<Package name="Distribute Inventory Report" nameID="4C1">
				<Documentation>Defines PIP</Documentation>
				<BusinessTransaction name="Distribute Inventory Report" nameID="DistributeInventoryReport_BT" isGuaranteedDeliveryRequired="true">
					<RequestingBusinessActivity name="Initiate Inventory Report Distribution" nameID="InitiateInventoryReportDistribution" isAuthorizationRequired="false" isIntelligibleCheckRequired="true" isNonRepudiationReceiptRequired="false" isNonRepudiationRequired="false" timeToAcknowledgeReceipt="PT2H" retryCount="3">
						<DocumentEnvelope name="Inventory Report Distribution" nameID="InventoryReportDistribution_DE" businessDocument='//BusinessDocument[@name="Inventory Report Distribution"]' businessDocumentIDREF="PIP4C1InventoryReportDistribution" isAuthenticated="none" isConfidential="transient" isTamperDetectable="none"/>
					</RequestingBusinessActivity>
					<RespondingBusinessActivity name="Process Inventory Report Distribution" nameID="ProcessInventoryReportDistribution"/>
				</BusinessTransaction>
				<!--REM: Binary Collaborations-->
				<BinaryCollaboration name="Distribute Inventory Report" nameID="DistributeInventoryReport_BC" initiatingRoleIDREF="InventoryInformationProviderID" preCondition="true" postCondition="true">
					<Documentation>The PIP Start State is comprised of the following conditions: a. TPA Approved and Exist In Backend System b. Sending Partner Exist as Party and Valid c. Receiving Partner Exist as Party d. Business Document Exist In Backend System, and Valid. </Documentation>
					<Documentation>The PIP End States are comprised of the following conditions: a. Acknowledgment of Receipt Exist In Backend System, and Valid b. Receiving Partner Authorized ; PIP Failure State: a. NOT Successful b. Retry Count Exceeded</Documentation>
					<Role name="Inventory Information Provider" nameID="InventoryInformationProviderID"/>
					<Role name="Inventory Information User" nameID="InventoryInformationUserID"/>
					<Start nameID="Start_ST" toBusinessState='../BusinessTransactionActivity[@name="Distribute Inventory Report"]' toBusinessStateIDREF="PIP4C1DistributeInventoryReport"/>
					<BusinessTransactionActivity name="Distribute Inventory Report" nameID="DistributeInventoryReport_BTA" businessTransaction='//BusinessTransaction[@name="Distribute Inventory Report"]' businessTransactionIDREF="DistributeInventoryReport_BT" fromRole='../Role[@name="InventoryInformationProvider"]' fromRoleIDREF="InventoryInformationProviderID" toRole='../Role[@name="InventoryInformationUser"]' toRoleIDREF="InventoryInformationUserID" isLegallyBinding="true" isConcurrent="false"/>
					<!--REM: The following shows a business transaction with two (one-pair) possible responses, each pair with a success and a failure.These are designed to reflect business level conditions only and not message level conditions such as Receipt Acknowledgements (ReceiptAck) and/or Notification of Failures (NoF). -->
					<!--REM:  Success Condition for Distribute Inventory Report-->
					<Success nameID="Success_ST" fromBusinessState='../BusinessTransactionActivity[@name="Distribute Inventory Report"]' conditionGuard="Success">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Success"'/>
					</Success>
					<!--REM:  Failure Condition for Distribute Inventory Report-->
					<Failure nameID="Failure_ST" fromBusinessState='../BusinessTransactionActivity[@name="Distribute Inventory Report"]' conditionGuard="BusinessFailure">
						<ConditionExpression expressionLanguage="XPath" expression='//BusinessServiceInformation/ProcessState="Failure"'/>
					</Failure>
				</BinaryCollaboration>
			</Package>
		</Package>
	</Package>
</ProcessSpecification>
