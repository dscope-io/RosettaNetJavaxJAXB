    <xs:import namespace="urn:rosettanet:specification:domain:Procurement:xsd:schema:02.29" schemaLocation="../Domain/Procurement/Procurement_02_29.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:Country:xsd:codelist:01.02" schemaLocation="../Universal/CodeList/ISO_Country_01_02.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:ProductIdentification:xsd:schema:01.04" schemaLocation="../Universal/ProductIdentification_01_04.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Procurement:ProductProcurementType:xsd:codelist:01.03" schemaLocation="../Domain/Procurement/CodeList/RN_ProductProcurementType_01_03.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Design:ABCCode:xsd:codelist:01.02" schemaLocation="../Domain/Design/CodeList/RN_ABCCode_01_02.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Shared:xsd:schema:01.17" schemaLocation="../Domain/Shared/Shared_01_17.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:Document:xsd:schema:01.12" schemaLocation="../Universal/Document_01_12.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:Dates:xsd:schema:01.03" schemaLocation="../Universal/Dates_01_03.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Design:xsd:schema:02.22" schemaLocation="../Domain/Design/Design_02_22.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Marketing:xsd:schema:02.19" schemaLocation="../Domain/Marketing/Marketing_02_19.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Design:ActionCode:xsd:codelist:01.03" schemaLocation="../Domain/Design/CodeList/RN_ActionCode_01_03.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:domain:Design:CatalogType:xsd:codelist:01.01" schemaLocation="../Domain/Design/CodeList/RN_CatalogType_01_01.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:ContactInformation:xsd:schema:01.04" schemaLocation="../Universal/ContactInformation_01_04.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:universal:PartnerIdentification:xsd:schema:01.16" schemaLocation="../Universal/PartnerIdentification_01_16.xsd"></xs:import>
    <xs:import namespace="urn:rosettanet:specification:system:StandardDocumentHeader:xsd:schema:01.23" schemaLocation="../System/StandardDocumentHeader_01_23.xsd"></xs:import>
